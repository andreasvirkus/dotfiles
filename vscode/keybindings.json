[
  {
      "key": "cmd+8",
      "command": "editor.action.commentLine",
      "when": "editorTextFocus"
  },
  {
      "key": "shift+cmd+7",
      "command": "-editor.action.commentLine",
      "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+z",
    "command": "redo"
  },
  {
    "key": "cmd+y",
    "command": "-redo"
  },
  {
    "key": "cmd+y",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+k",
    "command": "-editor.action.deleteLines",
    "when": "editorTextFocus"
  }
]
